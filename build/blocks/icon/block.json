{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gatherpress/icon",
  "version": "1.0.0",
  "title": "Icon",
  "description": "Adds customizable icons to visually enhance your content.",
  "category": "gatherpress",
  "icon": "format-image",
  "example": {},
  "attributes": {
    "icon": {
      "type": "string",
      "default": "nametag"
    },
    "iconColor": {
      "type": "string",
      "default": ""
    },
    "iconSize": {
      "type": "number",
      "default": 24
    }
  },
  "supports": {
    "align": [
      "left",
      "center",
      "right"
    ],
    "anchor": true,
    "html": false,
    "spacing": {
      "margin": true
    }
  },
  "textdomain": "gatherpress",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}