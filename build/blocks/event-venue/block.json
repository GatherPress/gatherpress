{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "gatherpress/event-venue",
  "version": "1.0.0",
  "title": "Event Venue",
  "category": "gatherpress",
  "icon": "location",
  "example": {},
  "description": "The block used for displaying the event's venue.",
  "attributes": {
    "mapShow": {
      "type": "boolean",
      "default": true
    },
    "mapZoomLevel": {
      "type": "number",
      "default": 10
    },
    "mapType": {
      "type": "string",
      "default": "m"
    },
    "mapHeight": {
      "type": "number",
      "default": 300
    }
  },
  "supports": {
    "align": [
      "wide"
    ],
    "html": true
  },
  "textdomain": "gatherpress",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./event-venue.js",
  "render": "file:./render.php"
}