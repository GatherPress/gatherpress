{
	"name": "gatherpress",
	"version": "0.33.3",
	"description": "Powering Communities with WordPress",
	"author": "",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"dates",
		"events"
	],
	"homepage": "https://github.com/GatherPress/gatherpress#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GatherPress/gatherpress.git"
	},
	"bugs": {
		"url": "https://github.com/GatherPress/gatherpress/issues"
	},
	"main": "index.js",
	"devDependencies": {
		"@anthropic-ai/claude-code": "^2.0.31",
		"@jest/globals": "^30.0.2",
		"@playwright/test": "^1.56.1",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@typescript-eslint/eslint-plugin": "^8.34.1",
		"@wordpress/api-fetch": "^7.25.0",
		"@wordpress/block-editor": "^14.20.0",
		"@wordpress/blocks": "^14.14.0",
		"@wordpress/components": "^29.11.0",
		"@wordpress/compose": "^7.25.0",
		"@wordpress/core-data": "^7.25.0",
		"@wordpress/data": "^10.25.0",
		"@wordpress/date": "^5.25.0",
		"@wordpress/dom-ready": "^4.25.0",
		"@wordpress/e2e-test-utils-playwright": "^1.25.0",
		"@wordpress/editor": "^14.25.0",
		"@wordpress/element": "^6.25.0",
		"@wordpress/env": "^10.37.0",
		"@wordpress/eslint-plugin": "^22.14.0",
		"@wordpress/i18n": "^5.25.0",
		"@wordpress/icons": "^10.25.0",
		"@wordpress/interactivity": "^6.25.0",
		"@wordpress/interactivity-router": "^2.25.0",
		"@wordpress/plugins": "^7.25.0",
		"@wordpress/scripts": "^30.18.0",
		"@wp-playground/cli": "^1.1.3",
		"classnames": "^2.5.1",
		"clsx": "^2.1.1",
		"dotenv": "^16.5.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"html-react-parser": "^5.2.5",
		"jest-sonar-reporter": "^2.0.0",
		"leaflet": "^1.9.4",
		"leaflet-gesture-handling": "^1.2.2",
		"lodash": "^4.17.21",
		"moment": "^2.30.1",
		"moment-timezone": "^0.6.0",
		"playwright": "^1.56.1",
		"prettier": "^3.6.2",
		"react-modal": "^3.16.3",
		"react-tooltip": "^5.29.1",
		"rimraf": "^6.0.1",
		"stylelint-config-standard": "^38.0.0",
		"uuid": "^11.1.0",
		"webpack-remove-empty-scripts": "^1.1.1"
	},
	"overrides": {
		"@wordpress/scripts": {
			"qs": ">=6.14.1"
		},
		"@wp-playground/cli": {
			"qs": ">=6.14.1"
		}
	},
	"scripts": {
		"build": "rimraf build && wp-scripts build --experimental-modules",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:css:fix": "wp-scripts lint-style --fix",
		"lint:js": "wp-scripts lint-js",
		"lint:js:fix": "wp-scripts lint-js --fix",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:md:js": "wp-scripts lint-md-js",
		"lint:php": "vendor/bin/phpcs --standard=phpcs.ruleset.xml --extensions=php --colors -s -p -v .",
		"lint:php:fix": "vendor/bin/phpcbf --standard=phpcs.ruleset.xml -p -v .",
		"lint:phpstan": "vendor/bin/phpstan analyze -vv --memory-limit=2G",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "npm run build && wp-scripts plugin-zip",
		"start": "wp-scripts start --experimental-modules",
		"screenshots:wporg": "WP_BASE_URL='http://127.0.0.1:9400/' wp-scripts test-playwright --config .github/scripts/wordpress-org-screenshots/playwright.config.ts",
		"screenshots:wporg:debug": "npm run screenshots:wporg -- --debug",
		"screenshots:wporg:ui": "npm run screenshots:wporg -- --ui",
		"test:e2e": "playwright test --config=playwright.config.js",
		"test:unit:js": "wp-scripts test-unit-js --coverage --testResultsProcessor=jest-sonar-reporter",
		"pretest:unit:php": "wp-env start --xdebug",
		"test:unit:php": "wp-env run tests-wordpress php -dxdebug.mode=coverage /var/www/html/wp-content/plugins/gatherpress/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/gatherpress/phpunit.xml.dist --coverage-clover=coverage.xml --coverage-html=build/coverage-report",
		"pretest:unit:php:multisite": "wp-env start --xdebug",
		"test:unit:php:multisite": "wp-env run tests-wordpress php -dxdebug.mode=coverage /var/www/html/wp-content/plugins/gatherpress/vendor/bin/phpunit -c /var/www/html/wp-content/plugins/gatherpress/phpunit-multisite.xml.dist --coverage-clover=coverage-multisite.xml --coverage-html=build/coverage-report-multisite",
		"wp-env": "wp-env",
		"playground": "./node_modules/@wp-playground/cli/wp-playground.js server",
		"playground:mount": "./node_modules/@wp-playground/cli/wp-playground.js server --mount=./:/wordpress/wp-content/plugins/gatherpress",
		"claude": "npx @anthropic-ai/claude-code"
	}
}
