{
  "minCoverage": 80,
  "excludePatterns": [
    "*/vendor/*",
    "*/node_modules/*",
    "*/build/*",
    "*/test/*",
    "*/tests/*",
    "*gatherpress.php",
    "*/class-autoloader.php",
    "*/class-singleton.php",
    "*/requirements-check.php",
    "*/duplicate-check.php",
    "*/templates/*",
    "*/render.php",
    "*/e2e/*",
    ".github/**",
    "includes/data/*",
    "src/index.js",
    "src/editor.js",
    "src/profile/index.js",
    "src/settings/index.js",
    "src/blocks/*/index.js",
    "src/blocks/*/edit.js",
    "src/blocks/*/view.js",
    "src/blocks/*/save.js",
    "src/blocks/*/template.js",
    "src/blocks/*/templates/*.js",
    "src/blocks/*/panels/**/*",
    "src/blocks/*/types/**/*",
    "src/panels/**/*",
    "src/modals/**/*",
    "src/variations/**/*",
    "src/components/**/*",
    "src/supports/**/*",
    "src/stores/index.js",
    "src/helpers/urlrewrite.js",
    "src/helpers/interactivity.js"
  ],
  "allowLowCoverage": [
    "includes/core/classes/class-setup.php",
    "includes/core/classes/class-event-setup.php"
  ],
  "allowLowCoverageReason": "Contains multisite-specific code that cannot be tested in single-site environment"
}
